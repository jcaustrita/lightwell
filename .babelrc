{
	presets: [
		["@babel/preset-env", {
			useBuiltIns: "entry"
		}]
	],
	plugins: [
		["@babel/plugin-proposal-export-default-from"],
		["@babel/plugin-proposal-optional-chaining"],
		["import-glob"],
		["transform-function-bind"],
		["module-resolver", {
			"alias": {
				"@": "./src"
			}
		}]
	]
}
